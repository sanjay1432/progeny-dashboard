{
  "name": "progeny-dashboard",
  "version": "2.1.0",
  "description": "Progeny dashboard",
  "main": "index.js",
  "scripts": {
    "start": "react-app-rewired start",
    "workbox-build-inject": "node workbox-build-inject.js",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/opex-dashboard.scss src/assets/css/opex-dashboard.css",
    "minify-sass": "node-sass src/assets/scss/opex-dashboard.scss src/assets/css/opex-dashboard.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/opex-dashboard.scss src/assets/css/opex-dashboard.css --source-map true",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.jsx|*.js|*.scss|*.css)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.jsx|*.js|*.scss|*.css)\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@casl/ability": "^5.2.0",
    "@casl/react": "^2.2.2",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@reduxjs/toolkit": "^1.5.0",
    "axios": "^0.21.1",
    "bootstrap": "4.6.0",
    "classnames": "2.2.6",
    "customize-cra": "^1.0.0",
    "echarts": "5.0.2",
    "echarts-for-react": "3.0.1",
    "file-saver": "^2.0.2",
    "html2canvas": "^1.0.0-rc.7",
    "jspdf": "^2.2.0",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "lodash": "^4.17.20",
    "moment": "2.29.1",
    "node-sass": "^4.14.1",
    "nouislider": "14.6.3",
    "react": "^17.0.1",
    "react-app-rewired": "^2.1.8",
    "react-custom-scrollbars": "^4.2.1",
    "react-d3-speedometer": "^0.14.1",
    "react-datetime": "2.16.3",
    "react-dom": "16.14.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^7.2.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "^4.0.1",
    "react-select": "^3.1.0",
    "react-table": "^7.6.3",
    "react-validation": "^3.0.7",
    "react-virtualized": "^9.22.2",
    "reactstrap": "8.9.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rsuite": "^4.8.4",
    "validator": "^13.1.17",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.3",
    "workbox-broadcast-update": "^5.1.3",
    "workbox-cacheable-response": "^5.1.3",
    "workbox-core": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-google-analytics": "^5.1.3",
    "workbox-navigation-preload": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-range-requests": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-streams": "^5.1.3",
    "workbox-window": "^6.1.2",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@types/googlemaps": "3.39.2",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.9.19",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-prettier": "^3.3.1",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "jquery": "^3.5.1",
    "prettier": "^2.2.1",
    "typescript": "3.7.5",
    "workbox-build": "^6.1.2",
    "husky": "3.0.5",
    "lint-staged": "9.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
